{
  "type": "mongodb",
  "host": "host.docker.internal",
  "port": 27017,
  "database": "startnode1",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/models/*.ts"
  ],
  "migrations": [ "src/migration/**/*.ts"
  ],
  "subscribers": [ "src/subscriber/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/models", "migrationsDir": "src/migration", "subscribersDir": "src/subscriber"
  }
}